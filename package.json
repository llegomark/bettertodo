	  "eslint": "^8.54.0",
	  "postcss": "^8.4.31",
	  "prettier": "^3.1.0",
	  "tailwindcss": "^3.3.5",
	  "ts-node": "^10.9.1",
	  "typescript": "^5.2.2",
	  "zod": "^3.22.4"
	},
	"name": "BetterTodo",
	"prisma": {
	  "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
